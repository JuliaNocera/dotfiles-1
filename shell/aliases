# GIT
alias gcm='git commit -m'
alias gcM='gcm'
alias gmc='gcm'
alias fetch='git fetch'
alias pull='git pull'
alias push='git push'
alias gs='git status'
alias stash='git stash'
alias pop='git status pop'

# MISC
alias vim='nvim'
alias code='cd $HOME/code'

webhooks() {
  if [ -z "$1" ]; then
    ngrok http -subdomain=jerelmiller 80
  else
    ngrok http -subdomain=jerelmiller "$1"
  fi
}
